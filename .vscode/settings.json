{
    "protoc": {
        "path": "/usr/local/bin/protoc",
        "options": [
            "--proto_path=${workspaceRoot}/ag",
            "--proto_path=${env.HOME}/go/pkg/mod/github.com/gogo/protobuf@v1.3.1",
        ],
    },
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast"
    ],
    "cSpell.words": [
        "Debugf",
        "Gorm",
        "Repos",
        "quickfeed",
        "autograde",
        "covermode",
        "coverprofile",
        "gopath",
        "goveralls",
        "grpc",
        "jinzhu",
        "opsys",
        "repotoken",
        "scms",
        "sqlite"
    ]
}